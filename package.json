  "scripts": {
    "android": "nx run-android",
    "beta-ios": "(cd apps/habit-app/ios && bundle exec fastlane beta)",
    "beta-android": "(cd apps/habit-app/android && bundle exec fastlane beta)",
    "check-coverage": "(npm run test-all -- --configuration=ci --skip-nx-cache && npm run test-coverage) || open coverage/combine-coverage/index.html",
    "e2e-build-android": "nx affected --target=e2e-build-android",
    "e2e-build-ios": "nx affected --target=e2e-build-ios",
    "e2e-test-android": "nx affected --target=e2e-test-android",
    "e2e-test-ios": "nx affected --target=e2e-test-ios",
    "generate": "nx run-many --target=generate --all",
    "ios": "nx run-ios",
    "lint": "nx affected:lint --max-warnings=0",
    "postinstall": "patch-package",
    "prestorybook-start": "cd apps/storybook-app/src && sb-rn-get-stories",
    "start": "nx serve",
    "storybook-watcher": "cd apps/storybook-app/src && sb-rn-watcher",
    "storybook-android": "nx run storybook-app:run-android",
    "storybook-ios": "nx run storybook-app:run-ios",
    "storybook-start": "nx run storybook-app:start",
    "test": "nx affected:test",
    "test-all": "rm -rf coverage && npx nx run-many --all --target=test",
    "test-coverage": "node tools/combine-coverage-final.js && nyc merge coverage coverage/combine-coverage/coverage.json && nyc report -t coverage/combine-coverage --report-dir coverage/combine-coverage --reporter=html"
  },
